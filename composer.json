{
    "name": "lvcms/admin",
    "type": "library",
    "description": ":后台管理系统 admin 使用自动构建 html",
    "keywords": [
        "lvcms",
        "admin"
    ],
    "homepage": "https://github.com/lvcms/admin",
    "license": "MIT",
    "authors": [
        {
            "name": "bigrocs",
            "email": "bigrocs@qq.com",
            "homepage": "http://www.lece.vip",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "~5.7",
        "lvcms/core": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Lvcms\\Admin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lvcms\\Admin\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Lvcms\\Admin\\ServiceProvider"
            ]
        },
        "lvcms": {
            "name": "admin",
            "title": "后台管理系统",
            "description": "后台管理系统 admin 使用自动构建html",
            "author": "BigRocs",
            "version": "1.0.1",
            "providers": [
                "Lvcms\\Admin\\ServiceProvider"
            ],
            "install": "lvcms:admin:install",
            "uninstall": "lvcms:admin:uninstall"
        }
    },
    "config": {
        "sort-packages": true
    }
}
